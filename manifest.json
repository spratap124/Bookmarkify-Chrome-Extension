{
    "manifest_version": 2,
    "name": "Bookmarkify",
    "description": "Bookmark anywhere on a web page",
    "version": "1.0.2",
    "icons": {
        "16":"img/icon_16.png",
        "48":"img/icon_48.png",
        "128": "img/icon_128.png"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["./js/content.js"],
        "css": ["./css/style.css"]
    }],
    "browser_action": {
        "default_icon": "img/icon_16.png",
        "default_popup": "popup.html"
    }
}